@using WebApp.web.Models.ViewModel
@model List<ProductListComponentViewModel>


<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Name</th>

            <th>Açıklama</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Describtion</td>
            </tr>
        }
    </tbody>
</table>

